<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">


    <!--    Create helloSpring bean, wo don't need set this one anymore, injection;-->
    <bean id="helloSpring" class="com.yinom.pdd.spring.service.HelloSpring"/>
    <bean id="job" class="com.yinom.pdd.spring.ioc.Job">
        <property name="tester" ref="lida"/>
    </bean>
    <bean id="zhangming" class="com.yinom.pdd.spring.ioc.Zhangming"/>
    <bean id="lida" class="com.yinom.pdd.spring.ioc.Lida"/>


    <!--properties DI-->
    <bean id="employee" class="com.yinom.pdd.spring.model.Employee">
        <property name="id" value="1"/>
        <property name="name" value="Tom"/>
        <property name="salary" value="1000"/>
    </bean>
</beans>